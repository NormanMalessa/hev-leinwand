{
    "compilerOptions": {
        "outDir": "./public/js/",
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        // required for FOSJsRoutingBundle
        "esModuleInterop": true
    },
    "include": [
        "./src-js/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}